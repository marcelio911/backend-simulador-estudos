{
  "name": "backend-simulator-microservices",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "firebase:build": "yarn pre-firebase && webpack  --mode=development",
    "firebase:serve": "yarn pre-firebase && webpack --mode=development && firebase serve --only functions",
    "firebase:emulator": "yarn pre-firebase && webpack --mode=development && firebase emulators:start --only functions",
    "firebase:dev": "yarn pre-firebase && nodemon -w *.ts --exec npm run firebase:serve",
    "firebase:deploy": "yarn pre-firebase && webpack --mode=production && firebase deploy --only functions",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "pre-firebase": "yarn build && export ENVFILE=.env && && \\cp -a functions_config/. functions/ && cd functions/ && pnpm install && rm -f pnpm-lock.yaml",
    "pre-start": "export ENVFILE=.env && \\cp -a functions_config/. functions/ && cd functions/ && rm -f pnpm-lock.yaml",
    "mongoatlas:deploy": "yarn pre-start && cd functions/ && appservices push",
    "start": "yarn pre-start && nest start",
    "start:dev": "yarn pre-start && nest start --watch",
    "start:debug": "yarn pre-start && nest start --debug --watch",
    "start:prod": "yarn pre-start && yarn build && node functions/main",
    "google:cloud:run": "pnpm install && pnpm run build && node functions/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:linux": "./build-linux.sh"
  },
  "bin": "./functions/main.js",
  "pkg": {
    "scripts": "./functions/main.js",
    "assets": [
      ".env",
      "node_modules/.pnpm/pdf-parse@1.1.1/**",
      "node_modules/.pnpm/mongoose@8.4.0/**"
    ]
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@types/express": "^4.0.37",
    "@types/passport-google-oauth20": "^2.0.16",
    "axios": "0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "fs": "0.0.1-security",
    "mammoth": "^1.7.2",
    "mongoose": "^8.4.0",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "openai": "^4.47.1",
    "openai-node": "^1.0.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.4.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "7.3.0",
    "@nestjs/testing": "*",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "*",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.15.4",
    "jest": "^29.5.0",
    "pkg": "^5.8.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}